   <constraint>
     <cname>lmFuelConstraint</cname>
     <cvalue>
       <row>0</row>
       <cdata>LOR = no &amp; lmFuel = NA</cdata>
     </cvalue>
     <cvalue>
       <row>1</row>
       <cdata>LOR = yes &amp; !(lmFuel = NA)</cdata>
     </cvalue>
   </constraint>
   <constraint>
     <cname>LORconstraint</cname>
     <cvalue>
       <row>0</row>
       <cdata>LOR = yes &amp; moonArrival = orbit</cdata>
     </cvalue>
     <cvalue>
       <row>1</row>
       <cdata>LOR = no</cdata>
     </cvalue>
   </constraint>
   <constraint>
     <cname>lmcmcrew</cname>
     <cvalue>
       <row>0</row>
       <cdata>cmCrew &gt;= lmCrew</cdata>
     </cvalue>
   </constraint>
   <constraint>
     <cname>EORconstraint</cname>
     <cvalue>
       <row>0</row>
       <cdata>EOR = yes and earthLaunch = orbit</cdata>
     </cvalue>
     <cvalue>
       <row>1</row>
       <cdata>EOR = no</cdata>
     </cvalue>
   </constraint>
   <constraint>
     <cname>moonLeaving</cname>
     <cvalue>
       <row>0</row>
       <cdata>LOR = yes &amp; moonDeparture = orbit</cdata>
     </cvalue>
     <cvalue>
       <row>1</row>
       <cdata>LOR = no</cdata>
     </cvalue>
   </constraint>
   <constraint>
     <cname>lmexists</cname>
     <cvalue>
       <row>0</row>
       <cdata>LOR = no &amp; lmCrew = 0</cdata>
     </cvalue>
     <cvalue>
       <row>1</row>
       <cdata>LOR = yes &amp; lmCrew &gt; 0</cdata>
     </cvalue>
   </constraint>
